<p id="notice"><%= notice %></p>

<h1>Data</h1>

<h4>Import file!</h4>
<%= form_tag import_data_path, multipart: true do %>
  <%= file_field_tag :file%>
  <%= submit_tag 'Загрузить файл'%>
<%end%>

<table>
  <thead>
    <tr>
      <th>X</th>
      <th>Y</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data.each do |datum| %>
      <tr>
        <td><%= datum.x %></td>
        <td><%= datum.y %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>